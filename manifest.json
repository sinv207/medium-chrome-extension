{
    "name": "GoEN Proxy",
    "description": "Free proxy tool developed to help you access websites that may be restricted in your location.",
    "version": "1.0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["activeTab", "contextMenus"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/proxer16.png",
            "32": "/images/proxer32.png",
            "48": "/images/proxer48.png",
            "128": "/images/proxer128.png"
        }
    },
    "icons": {
        "16": "/images/proxer16.png",
        "32": "/images/proxer32.png",
        "48": "/images/proxer48.png",
        "128": "/images/proxer128.png"
    }
}
